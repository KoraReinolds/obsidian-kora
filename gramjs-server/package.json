{
	"name": "gramjs-server",
	"version": "1.0.0",
	"description": "GramJS server for Obsidian Kora plugin",
	"type": "module",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc",
		"dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\" --names \"tsc,server\" --prefix-colors \"cyan,magenta\"",
		"dev:userbot": "concurrently \"tsc --watch\" \"nodemon dist/server.js -- --port=8125 --mode=userbot\" --names \"tsc,userbot\" --prefix-colors \"cyan,green\"",
		"dev:dual": "concurrently \"tsc --watch\" \"nodemon dist/server.js -- --port=8124 --mode=bot\" \"nodemon dist/server.js -- --port=8125 --mode=userbot\" --names \"tsc,bot,userbot\" --prefix-colors \"cyan,magenta,green\"",
		"watch": "tsc --watch",
		"start": "node dist/server.js",
		"start:userbot": "node dist/server.js --port=8125 --mode=userbot",
		"start:dual": "concurrently \"node dist/server.js --port=8124 --mode=bot\" \"node dist/server.js --port=8125 --mode=userbot\" --names \"bot,userbot\" --prefix-colors \"magenta,green\"",
		"auth": "node dist/gramjs-auth.js"
	},
	"dependencies": {
		"@qdrant/js-client-rest": "^1.15.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.0",
		"express": "^4.18.2",
		"input": "^1.0.1",
		"openai": "^4.68.4",
		"telegram": "^2.22.2",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.0",
		"@types/uuid": "^9.0.7",
		"concurrently": "^9.2.0",
		"nodemon": "^3.1.10",
		"typescript": "^5.3.2"
	}
}
